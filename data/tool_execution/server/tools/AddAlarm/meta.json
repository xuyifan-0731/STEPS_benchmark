{
    "name": "AddAlarm",
    "description": "This API sets an alarm for the given time.",
    "parameters": [
        {
            "name": "username",
            "type": "str",
            "description": "Username of the user's device"
        },
        {
            "name": "password",
            "type": "str",
            "description": "Password of the user's device"
        },
        {
            "name": "time",
            "type": "str",
            "description": "The time for alarm. Format: YYYY-MM-DD hh:mm:ss'"
        }
    ],
    "return": "success or failed",
    "example": [
        {
            "input": {
                "username": "JohnDoe",
                "password": "pass123",
                "time": "2023-05-15 09:50:00"
            },
            "output": "The alarm has been successfully scheduled for 2023-05-15 09:50:00."
        }
    ]
}